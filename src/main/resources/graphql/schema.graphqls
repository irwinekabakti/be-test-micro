type Car {
    id: ID!
    brand: String!
    model: String!
    year: Int!
    image: String!
}

type Query {
    getAllCars: [Car]!
    getCarById(id: ID!): Car
}

type Mutation {
    saveCar(brand: String!, model: String!, year: Int!, image: String): Car!
    editCar(id: ID!, brand: String, model: String, year: Int, image: String): Car
    deleteCar(id: ID!): String!
}
